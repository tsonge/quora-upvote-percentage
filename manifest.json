{
  "name": "Quora Upvote Percentage",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://quora.com/*", "https://www.quora.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": { 
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "browser_action": {
    "default_icon": "icon48.png"
  }
}